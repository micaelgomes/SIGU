{\n    await api.post<AuthState>('/users', {\n      name,\n      email,\n      password,\n    });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@sigu:t0k3n');\n    localStorage.removeItem('@sigu:user');\n\n    setData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ signIn, signUp, signOut, user: data.user }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nconst useAuth = (): AuthContextData => {\n  const context = useContext(AuthContext);\n\n  if (!context) throw new Error('useAuth must be used in auth context.');\n\n  return context;\n};\n\nexport { AuthProvider, useAuth };\n","/home/micaellgoms/Code/mateus-sigu/src/components/Card/index.tsx",[],"/home/micaellgoms/Code/mateus-sigu/src/components/Card/styled.ts",[],"/home/micaellgoms/Code/mateus-sigu/src/components/UserMenu/styled.ts",[],"/home/micaellgoms/Code/mateus-sigu/src/components/UserMenu/index.tsx",[],"/home/micaellgoms/Code/mateus-sigu/src/components/BottomView/index.tsx",[],"/home/micaellgoms/Code/mateus-sigu/src/components/BottomView/styled.ts",[],"/home/micaellgoms/Code/mateus-sigu/src/context/bottomView.tsx",["194"],"import React, {\n  createContext,\n  Dispatch,\n  SetStateAction,\n  useCallback,\n  useContext,\n  useState,\n} from 'react';\nimport BottomView from '../components/BottomView';\n\nexport interface UserModel {\n  id: string;\n  email: string;\n  name: string;\n  avatar?: string;\n  createAt?: string;\n  updateAt?: string;\n}\n\ninterface BottomViewData {\n  stateBottomView: boolean;\n  toogleBottomView(): void;\n  clearUser(): void;\n  setUser: Dispatch<SetStateAction<UserModel>>;\n}\n\nconst BottomViewContext = createContext<BottomViewData>({} as BottomViewData);\n\nconst BottomViewProvider: React.FC = ({ children }) => {\n  const [opened, setOpened] = useState<boolean>(false);\n  const [user, setUser] = useState<UserModel>({} as UserModel);\n\n  console.log(user);\n\n  const clearUser = useCallback(() => set